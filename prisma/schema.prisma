// This is your Prisma schema file

generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// User model
model User {
  id       Int    @id @default(autoincrement())
  login    String @unique
  password String
}
